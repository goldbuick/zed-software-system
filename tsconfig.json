{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "/zss/*": [
        "./zss/*"
      ],
    },
    "lib": [
      "dom",
      "esnext",
      "webworker",
      "dom.iterable",
    ],
    "target": "es6",
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true,
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".eslintrc.js",
    ".prettierrc",
    "parcel.d.ts",
    "tsconfig.json",
    "tailwind.config.js",
    "zss/**/*",
  ]
}