{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "fs:default",
    "log:default",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "$APPDATA/archive/*"
        }
      ]
    }
  ]
}