{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "zed-software-system",
  "version": "0.1.0",
  "identifier": "com.goldbuick.zed-cafe",
  "build": {
    "frontendDist": "../cafe/dist",
    "devUrl": "https://localhost:7777",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "app": {
    "windows": [
      {
        "title": "zed-cafe",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}